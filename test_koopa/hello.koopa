global %global_x = alloc i32, zeroinit
global %global_init = alloc i32, 1

decl @getint(): i32

decl @getch(): i32

decl @putint(i32)

decl @putch(i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%entry:
  %0 = alloc i32
  %1 = load %global_x
  call @putint(%1)
  call @putch(32)
  call @putint(10)
  call @putch(32)
  call @putint(11)
  call @putch(32)
  %2 = load %global_init
  call @putint(%2)
  call @putch(10)
  store 0, %0
  jump %end_bb

%end_bb:
  %3 = load %0
  ret %3
}
