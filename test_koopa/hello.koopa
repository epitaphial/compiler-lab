fun @main(): i32 {
%entry:
  %0 = alloc i32
  %1 = alloc i32
  store 1, %1
  %2 = alloc i32
  store 2, %2
  %3 = alloc i32
  jump %bb0

%end_bb:
  %4 = load %0
  ret %4

%bb0:
  %5 = load %1
  %6 = alloc i32
  store 0, %6
  %7 = ne %5, 0
  br %7, %bb1, %bb2

%bb1:
