fun @main(@b: i32, @c: i32, @d: i32) {
%entry:
  %0 = alloc i32
  store @b, %0
  %1 = alloc i32
  store @c, %1
  %2 = alloc i32
  store @d, %2
  %3 = alloc i32
  %4 = load %3
  br %4, %bb0, %bb3

%bb0:
  %5 = load %3
  br %5, %bb1, %bb2

%bb1:
  %6 = load %0
  store %6, %3
  jump %bb2

%bb2:
  jump %bb3

%bb3:
  ret
}
